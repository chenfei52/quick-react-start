{
    "presets": [
        "@babel/react",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": "58"
                },
                "useBuiltIns": "entry",
                "corejs": {
                    "version": 3,
                    "proposals": true
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ]
}